<hc:Window x:Class="WinBooster_WPF.Loading"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WinBooster_WPF"
        xmlns:hc="https://handyorg.github.io/handycontrol"
        mc:Ignorable="d"
        ShowMinButton="False"
        ShowMaxButton="False"
        ShowCloseButton="False"
        Closing="Window_Closing"
        Title="Loading" Height="170" Width="80">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Vertical" Margin="5">
            <hc:WaveProgressBar Value="60"/>
        </StackPanel>
    </Grid>
</hc:Window>
